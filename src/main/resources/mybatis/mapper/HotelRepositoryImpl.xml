<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="team.cms.repository.HotelRepository">

    <select id="getHotelByName" resultType="team.cms.entity.Hotel">
        select * from hotel where name=#{name}
    </select>
    <insert id="add">
        insert into hotel() values ()
    </insert>
    <delete id="removeByName">
        delete from hotel where name=#{name};
    </delete>
    <update id="updateHotel">
        update hotel set
    </update>
</mapper>