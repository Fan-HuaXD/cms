<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="team.cms.repository.ConferenceRepository">

    <select id="getConferenceByName" resultType="team.cms.entity.Conference">
        select * from conference where name=#{name}
    </select>
    <insert id="add">
        insert into conference() values ()
    </insert>
    <delete id="removeByName">
        delete from conference where name=#{name};
    </delete>
    <update id="updateConference">
        update conference set
    </update>
</mapper>